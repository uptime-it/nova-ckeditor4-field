{
    "name": "uptime-it/nova-ckeditor4-field",
    "description": "This nova package allows you to use CKEditor 4 for text areas.",
    "keywords": [
        "laravel",
        "nova",
        "ckeditor"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Web Communications",
            "email": "web@wayne.edu"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Waynestate\\Nova\\CKEditor4Field\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waynestate\\Nova\\CKEditor4Field\\CKEditor4FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "chmod +x php-cs-fixer-script.sh && ./php-cs-fixer-script.sh"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}